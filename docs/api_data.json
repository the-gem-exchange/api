            "field": "sex",
            "description": ""
          },
          {
            "group": "Parameter",
            "type": "String",
            "allowedValues": [
              "\"myo\"",
              "\"batch\"",
              "\"auction\""
            ],
            "optional": false,
            "field": "type",
            "description": ""
          },
          {
            "group": "Parameter",
            "type": "String",
            "allowedValues": [
              "\"common\"",
              "\"uncommon\"",
              "\"rare\"",
              "\"legendary\""
            ],
            "optional": false,
            "field": "rarity",
            "description": ""
          },
          {
            "group": "Parameter",
            "type": "String",
            "optional": false,
            "field": "species",
            "description": "<p><code>starweaver</code>, <code>starshooter</code>, etc.</p>"
          },
          {
            "group": "Parameter",
            "type": "String",
